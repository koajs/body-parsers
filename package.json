    "@types/supertest": "^6.0.3",
    "c8": "^10.1.3",
    "koa": "^3.0.1",
    "rimraf": "^6.0.1",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "ts-standard": "^12.0.2",
    "tsup": "^8.5.0",
